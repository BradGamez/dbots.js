{
  "name": "dbots",
  "version": "1.3.0",
  "description": "Discord bot list poster and stats retriever",
  "main": "./src/index",
  "types": "./typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbots-pkg/dbots.js.git"
  },
  "scripts": {
    "docs": "node scripts/docs.js && docgen --source src --custom docs/index.yml --output docs/docs.json",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "gpr-setup": "node scripts/gpr.js",
    "tsd-jsdoc": "jsdoc -t node_modules/tsd-jsdoc/dist -r src",
    "tslint": "tslint ./typings/*.d.ts",
    "tslint:fix": "tslint ./typings/*.d.ts --fix",
    "types": "npm run tsd-jsdoc && mv out/types.d.ts typings/index.d.ts && rm out --dir && npm run tslint:fix",
    "webpack": "webpack",
    "prepublishOnly": "npm run eslint:fix && npm run tslint:fix && NODE_ENV=production npm run webpack"
  },
  "unpkg": "./webpack/dbots.min.js",
  "jsdelivr": "./webpack/dbots.min.js",
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "discord bots",
    "discord bot",
    "wrapper"
  ],
  "author": "Snazzah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dbots-pkg/dbots.js/issues"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.8",
    "discord.js-docgen": "github:discordjs/docgen",
    "dtslint": "^3.3.0",
    "eslint": "^6.5.1",
    "terser-webpack-plugin": "^2.3.5",
    "tsd-jsdoc": "github:EndBug/tsd-jsdoc",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
