{
  "name": "dbots",
  "version": "1.3.0",
  "description": "Discord bot list poster and stats retriever",
  "main": "./src/index",
  "types": "./typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Snazzah/dbots.js.git"
  },
  "scripts": {
    "docs": "docgen --source src --custom docs/index.yml --output docs/docs.json",
    "eslint": "eslint ./src",
    "gpr-setup": "node scripts/gpr.js",
    "hint": "jshint ./src",
    "lint": "npm run eslint && npm run hint"
  },
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "discord bots",
    "discord bot",
    "wrapper"
