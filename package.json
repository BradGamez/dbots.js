  "author": "Snazzah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Snazzah/dbots.js/issues"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.8",
    "discord.js-docgen": "github:discordjs/docgen",
    "eslint": "^6.5.1",
    "jshint": "^2.10.2"
  }
}
